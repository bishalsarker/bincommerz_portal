    "build-pm": "ng build product-management --prod --output-path dist/product-management --base-href /product-management/",
    "build-om": "ng build order-management --prod --output-path dist/order-management --base-href /order-management/",
    "build-cm": "ng build content-management --prod --output-path dist/content-management --base-href /content-management/",
    "build-am": "ng build account-management --prod --output-path dist/accounts --base-href /accounts/",
    "build": "npm run build-dashboard && npm run build-pm && npm run build-om && npm run build-cm && npm run build-am",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
